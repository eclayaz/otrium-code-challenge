{
    "require": {
        "php-di/php-di": "^6.3",
        "nikic/fast-route": "^1.3",
        "nesbot/carbon": "^2.54",
        "doctrine/dbal": "^3.1",
        "league/csv": "^9.7",
        "webmozart/assert": "^1.10",
        "doctrine/orm": "^2.10",
        "symfony/cache": "^5.3"
    },
    "require-dev": {
        "phpmd/phpmd": "@stable",
        "phpunit/phpunit": "^9.0",
        "squizlabs/php_codesniffer": "3.5.*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AppTest\\": "test/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
